{
    "name": "rtuin/carbon-normalizer",
    "description": "Normalizes to (and from) Carbon and native DateTime objects",
    "type": "library",
    "keywords": ["symfony", "serializer", "normalizer", "carbon"],
    "require": {
        "nesbot/carbon": "^2.0",
        "symfony/serializer": "^5.0 || ^6.0",
        "symfony/property-info": "^5.0 || ^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "illuminate/support": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rtuin\\Normalizer\\": "src/Normalizer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rtuin\\Normalizer\\Tests\\": "src/Normalizer"
        }
    },
    "authors": [
        {
            "name": "Richard Tuin",
            "email": "richardtuin@gmail.com"
        },
        {
            "name": "Sander Krul"
        }
    ]
}
